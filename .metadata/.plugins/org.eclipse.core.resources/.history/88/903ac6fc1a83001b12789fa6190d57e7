package March12;

public class Fifth 
{
	public String repeatEnd(String str,int n)
	{
		int len=str.length();
		int i=len-n;
		String sub=str.substring(i);
		System.out.println(sub);
		for(int j=1;j<n;j++)
		{
			sub=sub+sub;
		}
		return sub;
	}
	public static void main(String[] args) 
	{
		Fifth f=new Fifth();
		f.repeatEnd("Hello",3);
	}

}
