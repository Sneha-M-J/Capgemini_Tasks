package day5.string;

//Task 2
public class StringRemoveVowels 
{
	public static void main(String[] args) 
	{
		//remove vowels
		String str="Welcome";
		int len=str.length();
		for(int i=0;i<len;i++)
		{
			char ch=str.charAt(i);
			if(ch!='A' && ch!='E'  && ch!='I'  && ch!='O'  && ch!='U'  && ch!='a'  && ch!='e'  && ch!='i'  && ch!='o'  && ch!='u')
			{
				System.out.print(ch);
			}
		}
		
		//Task 3
		//remove duplicate char
		String str2="apple";
	}

}
