package Lab1;
import java.util.Scanner;

class Fibonacci
{
	void nonrec(int n)
	{
		int a=1,b=1;
		int c=0;
		for(int i=1;i<n-1;i++)
		{
			c=a+b;
			a=b;
			b=c;
		}
		System.out.println(c);
	}
}

public class Exercise3 
{
	
	public static void main(String[] args) 
	{
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter the number");
		int n=sc.nextInt();
		Fibonacci f=new Fibonacci();
		f.nonrec(n);
	}
}
