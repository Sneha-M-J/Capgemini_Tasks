package March12;

class Fruit 
{
	String name;
	String taste;
	public void eat()
	{
		System.out.println("Name : "+name);
		System.out.println("Taste : "+taste);
	}
}

class Apple extends Fruit
{
	public void eat()
	{
		System.out.println("Name : apple");
		System.out.println("Taste : Good taste");
	}
}

class Orange extends Fruit
{
	public void eat()
	{
		System.out.println("Name : orange");
		System.out.println("Taste : Sour taste");
	}
}
public class First 
{
	public static void main(String[] args) 
	{
		Apple a=new Apple();
		a.eat();
		Orange o=new Orange();
		o.eat();
	}
}
